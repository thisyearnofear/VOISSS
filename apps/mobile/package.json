{
  "name": "@voisss/mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_OPTIONS='--max-old-space-size=4096' expo start --tunnel",
    "start-web": "NODE_OPTIONS='--max-old-space-size=4096' expo start --web",
    "start-web-dev": "NODE_OPTIONS='--max-old-space-size=4096' DEBUG=expo* expo start --web --tunnel",
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' expo start",
    "dev:tunnel": "NODE_OPTIONS='--max-old-space-size=4096' expo start --tunnel",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' expo export",
    "lint": "eslint .",
    "clean": "rm -rf .expo .turbo node_modules",
    "postinstall": "node -e \"if (!process.env.EXPO_PUBLIC_PINATA_API_KEY) { console.log('\\n\\n⚠️  WARNING: IPFS credentials not configured!\\nPlease set EXPO_PUBLIC_PINATA_API_KEY and EXPO_PUBLIC_PINATA_API_SECRET in your .env file\\nSee .env.example for details\\n\\n')}\""
  },
  "dependencies": {
    "@base-org/account": "^2.4.0",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@tanstack/react-query": "5.90.2",
    "@voisss/shared": "workspace:*",
    "@voisss/ui": "workspace:*",
    "browserify-zlib": "0.2.0",
    "buffer": "6.0.3",
    "expo": "^54.0.29",
    "expo-audio": "^1.1.0",
    "expo-av": "~16.0.8",
    "expo-constants": "~18.0.12",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.19",
    "expo-splash-screen": "~31.0.12",
    "expo-standard-web-crypto": "3.0.8",
    "expo-status-bar": "~3.0.9",
    "https-browserify": "1.0.0",
    "lucide-react-native": "^0.561.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-crypto": "2.2.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-purchases": "9.5.1",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.2",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "url": "0.11.4",
    "viem": "^2.18.3",
    "vm-browserify": "1.1.2",
    "wagmi": "^2.18.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@types/react": "^19.1.17",
    "babel-plugin-module-resolver": "^5.0.2",
    "typescript": "~5.9.3"
  },
  "private": true
}

# ElevenLabs API Configuration
ELEVENLABS_API_KEY=your_api_key_here

# Server Configuration
PORT=5577
NODE_ENV=production

# Blockchain Configuration (for gasless transactions)
SPENDER_PRIVATE_KEY=your_spender_private_key_here
BASE_RPC_URL=https://mainnet.base.org
NEXT_PUBLIC_VOICE_RECORDS_CONTRACT=your_contract_address_here
NEXT_PUBLIC_SPENDER_ADDRESS=your_spender_public_address_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5433/voisss
DB_POOL_MAX=10

# Redis Configuration (for Bull queue)
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=your_password_here

# Export Service Configuration
EXPORT_TEMP_DIR=/tmp/voisss-exports
EXPORT_OUTPUT_DIR=/var/www/voisss-exports
EXPORT_PUBLIC_URL=http://localhost:5577

# Worker Configuration
WORKER_INSTANCES=2
WORKER_CONCURRENCY=2

# Optional: Override default model
# ELEVENLABS_MODEL_ID=eleven_multilingual_sts_v2